    "react-router-dom": "5.1.2",
    "react-snapshot": "^1.3.0",
    "react-spinkit": "3.0.0",
    "react-three-fiber": "4.0.22",
    "redux-injectors": "1.2.0",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "s3-easy-deploy": "^1.1.0",
    "sanitize.css": "11.0.0",
    "serverless-http": "^1.5.2",
    "styled-components": "4.4.0",
    "three": "0.114.0",
    "uid": "0.0.2",
    "warning": "4.0.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
